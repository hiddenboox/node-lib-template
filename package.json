{
  "name": "<lib-name>",
  "version": "<lib-version>",
  "description": "<lib-description>",
  "main": "dist/<lib-name>.cjs.js",
  "module": "dist/<lib-name>.esm.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --exit",
    "test:watch": "./node_modules/.bin/mocha --watch --watch-extensions js \"test/**/*.js\"",
    "build": "./node_modules/.bin/rollup -c",
    "dev": "./node_modules/.bin/rollup -c -w",
    "semantic-release": "./node_modules/.bin/semantic-release"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hiddenboox/node-lib-template.git"
  },
  "keywords": [
    "request",
    "api",
    "https"
  ],
  "author": "Sebastian Soboci≈Ñski <sebastian@soft-bind.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiddenboox/node-lib-template/issues"
  },
  "homepage": "https://github.com/hiddenboox/node-lib-template#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "mocha": "^7.0.1",
    "prettier": "1.19.1",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "semantic-release": "^17.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
